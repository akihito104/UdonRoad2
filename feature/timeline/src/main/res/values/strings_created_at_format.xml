<resources xmlns:tools="http://schemas.android.com/tools">
    <string name="created_now">now</string>
    <!-- plurals candidate is omitted because of in the case of 1 second ago,
     `now` is shown at Twitter.  -->
    <string name="created_seconds_ago" tools:ignore="PluralsCandidate">%1$d seconds ago</string>
    <plurals name="created_minutes_ago">
        <item quantity="one">%1$d minute ago</item>
        <item quantity="other">%1$d minutes ago</item>
    </plurals>
    <plurals name="created_hours_ago">
        <item quantity="one">%1$d hour ago</item>
        <item quantity="other">%1$d hours ago</item>
    </plurals>
    <string name="created_date">d MMM</string>
    <string name="created_year_date">d MMM yy</string>
</resources>
